    <script type="text/javascript">
        function factorial(n) {
    let result = 1;
   
    for (let i = 2; i <= n; i++) {
        result *= i;
    }
    
    return result;
}

const n = 5; 
const fact = factorial(n);
console.log("Факторіал числа " + n + "!: " + fact);


        </script>
</body>
</html>
