    <script type="text/javascript">
function printDigits(n) {
   
    while (n > 0) {
        
        const digit = n % 10;
        console.log(digit); 
        n = Math.floor(n / 10);
    }
}


const n = 123456; 
console.log("Цифри числа " + n + " від права до ліва:");
printDigits(n);


        </script>
