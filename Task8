    <script type="text/javascript">
function sumIntegers(a, b) {
    let sum = 0;
   
    for (let i = a; i <= b; i++) {
        sum += i;
    }
    return sum;
}


const a = 1; 
const b = 5; 
const result = sumIntegers(a, b);
console.log("Сума цілих чисел від " + a + " до " + b + " включно: " + result);


        </script>
