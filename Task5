    <script type="text/javascript">
function describeGrade(k) {
    switch (k) {
        case 1:
            return "погано";
        case 2:
            return "незадовільно";
        case 3:
            return "задовільно";
        case 4:
            return "добре";
        case 5:
            return "відмінно";
        default:
            return "помилка";
    }
}


const k = 3; 
const gradeDescription = describeGrade(k);
console.log("Оцінка: " + gradeDescription);
 



 

        </script>
</body>
</html>
